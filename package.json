{
  "version": "1.1.1",
  "license": "MIT",
  "description": "schema builder and validating",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "dev": "nodemon test.js",
    "build": "tsdx build",
    "test": "tsdx test --coverage",
    "lint": "tsdx lint src test",
    "format": "tsdx lint src test --fix",
    "prepare": "tsdx build && husky install",
    "analyze": "size-limit --why",
    "publish": "npm publish --access public"
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "homepage": "https://github.com/devSupporters/volder",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/devSupporters/volder.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "name": "volder",
  "author": "salah alhashmi",
  "module": "dist/volder.esm.js",
  "devDependencies": {
    "husky": "^7.0.0",
    "nodemon": "^2.0.13",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^3.9.10"
  }
}
